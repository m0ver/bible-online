$(function(){
	var corner={
		src:"{%TEMPLATES_DIR%}/images/circle.png",
		width:"7px",
		height:"7px",
		background:"#464646",
		positions: ["topleft","topright"]
	};

	$("#header ul li a.current").addCorners("topleft,topright",corner);
});
</script>
